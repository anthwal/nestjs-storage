{
  "name": "@anthwal/nestjs-storage",
  "version": "1.1.2",
  "description": "Nestjs file system wrapping flydrive",
  "author": "Arun Anthwal",
  "license": "MIT",
  "url": "https://github.com/anthwal/nestjs-storage",
  "scripts": {
    "jest": "jest",
    "build": "rimraf -rf dist && tsc -p ./tsconfig.lib.json",
    "lint": "DEBUG=eslint:eslint eslint --fix \"{lib,tests}/**/*.ts\"",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "jest --config ./tests/jest-e2e.json --runInBand",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@anthwal/flydrive": "^1.2.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@eslint/js": "^9.23.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/core": "^10.4.6",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/testing": "^10.4.6",
    "@types/jest": "^28.1.7",
    "@types/node": "^20.17.5",
    "console-testing-library": "^0.3.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^10.2.11",
    "prettier": "3.5.3",
    "reflect-metadata": "^0.1.13",
    "release-it": "14.11.8",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.28.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.10.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0",
    "@anthwal/flydrive": "^1.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthwal/nestjs-storage"
  },
  "publishConfig": {
    "access": "public"
  }
}
